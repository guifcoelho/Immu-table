{
  "name": "guifcoelho/json-models",
  "description": "Package for using immutable json models instead of regular SQL or NoSql databases with sintax similar to Laravel Eloquent",
  "license": "MIT",
  "keywords": [
    "json",
    "models",
    "Laravel",
    "Eloquent"
  ],
  "authors": [
    {
      "name": "Guilherme Freitas Coelho",
      "email": "coelhoxz@gmail.com"
    }
  ],
  "require": {
    "php": "~7.3",
    "illuminate/config": "5.8.*",
    "fzaninotto/faker": "^1.8",
    "halaxa/json-machine": "^0.3.2",
    "symfony/var-dumper": "^4.3",
    "symfony/finder": "^4.3",
    "jaschilz/php-coverage-badger": "^2.0"
  },
  "require-dev": {
    "phpunit/phpunit": "^8"
  },
  "autoload": {
    "psr-4": {
      "guifcoelho\\JsonModels\\": "src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "guifcoelho\\JsonModels\\Tests\\": "tests"
    }
  },
  "scripts": {
    "run-tests-win": ".\\vendor\\bin\\phpunit --coverage-clover tests\\report\\coverage.xml --coverage-html tests\\report\\coverage-html && composer build-coverage-badge-win",
    "run-tests-linux": "./vendor/bin/phpunit --coverage-clover tests/report/coverage.xml --coverage-html tests/report/coverage-html && composer build-coverage-badge-linux",
    "build-coverage-badge-win": ".\\vendor\\bin\\php-coverage-badger tests\\report\\coverage.xml tests\\report\\coverage.svg",
    "build-coverage-badge-linux": "./vendor/bin/php-coverage-badger tests/report/coverage.xml tests/report/coverage.svg"
  },
  "extra": {
    "branch-alias": {
      "dev-master": "0.1-dev"
    }
  },
  "minimum-stability": "stable"
}
